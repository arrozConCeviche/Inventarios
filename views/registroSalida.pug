extends layout
block content
  .row
    .col-9
    .col-3
      a.button.btn.btn-outline-primary(href='/registroSalida/nuevo') Nueva Salida de Producto
  br
  .row
    .table-responsive
      table.table.table-striped.table-sm
        thead
          tr
            th ID
            th Nombre Producto
            th Cantidad
            th Precio
            th Fecha
            th Total
        each venta, i in ventas
          each producto, i in venta.cuerpoSalida
            tbody
              td= venta.idRegistro
              td= producto.producto
              td= producto.cantidad
              td= asdfasd
              td= asd
              td= producto.precio
